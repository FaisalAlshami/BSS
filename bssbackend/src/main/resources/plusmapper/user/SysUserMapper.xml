<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neusoft.springboottest.user.mapper.SysUserMapper">




    <update id="updateManBuyerId" parameterType="SysUser"  useGeneratedKeys="true" keyProperty="USER_ID">
        update  sys_user set
        MAN_BUYER_ID=#{manBuyerId}
        where USER_ID=#{userId}
    </update>


</mapper>
